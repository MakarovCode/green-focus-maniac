{
  "name": "green-focus-maniac",
  "publisher": "DonOneFallen",
  "displayName": "Green Focus Maniac",
  "description": "VSCode theme using green as main color with extra tweaks to focus on the code",
  "version": "0.0.4",
  "license": "MIT",
  "icon": "img/icon.png",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Green Focus Maniac",
        "uiTheme": "vs-dark",
        "path": "./themes/Green Focus Maniac-color-theme.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MakarovCode/green-focus-maniac"
  },
  "scripts": {
    "package": "vsce package",
    "package:major": "vsce package --major",
    "package:minor": "vsce package --minor",
    "package:patch": "vsce package --patch",
    "publish": "vsce publish",
    "publish:major": "vsce publish --major",
    "publish:minor": "vsce publish --minor",
    "publish:patch": "vsce publish --patch"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.7.0"
  }
}
